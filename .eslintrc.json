{
    "extends": "standard",
    "plugins":[
        "react",
        "jsx-a11y",
        "import"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    }
}
